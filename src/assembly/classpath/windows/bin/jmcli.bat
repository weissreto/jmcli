@ECHO OFF

SET PROGRAM_BIN_DIR=%~dp0
SET PROGRAM_HOME_DIR=%PROGRAM_BIN_DIR%..
SET MAIN_CLASS=ch.rweiss.jmcli.JmCli
SET CLASS_PATH=%PROGRAM_HOME_DIR%\lib\*

IF NOT DEFINED JAVA_HOME (
  SET JAVA_EXECUTABLE=java.exe
) ELSE (
  SET JAVA_EXECUTABLE="%JAVA_HOME%\bin\java.exe"
)

%JAVA_EXECUTABLE% -cp %CLASS_PATH% -Djava.library.path=%PROGRAM_BIN_DIR% %MAIN_CLASS% %*